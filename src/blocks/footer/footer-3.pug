																		+IE-1({col_td: 2, width_col: '70', aling: 'center', valing: 'top'})
																		div(style="display: inline-block; width: 100%; max-width: 67px; vertical-align: top;")
																			<!--TABLE RIGHT-->
																			table(align="right" border="0" cellpadding="0" cellspacing="0" width="100%" style="width: 100%; max-width: 100%; border-collapse: collapse; mso-table-lspace: 0pt; mso-table-rspace: 0pt;")
																				tr
																					td(align="center" style="font-size:0;")
																						table(align="center" border="0" cellpadding="0" cellspacing="0" width="100%" style="width: 100%; max-width: 100%; border-collapse: collapse; mso-table-lspace: 0pt; mso-table-rspace: 0pt;")
																							tr
																								td(align="center")
