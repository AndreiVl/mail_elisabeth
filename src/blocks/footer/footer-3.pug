														+IE-1({col_td: 2, width_col: '283', aling: 'center', valing: 'top'})
														div(style="display: inline-block; width: 100%; max-width: 283px; vertical-align: top;")
															table(align="center" border="0" cellpadding="0" cellspacing="0" class="display-width-child" width="100%" style="width: 100%; max-width: 100%; border-collapse: collapse; mso-table-lspace: 0pt; mso-table-rspace: 0pt;")
																tr
																	td(align="center" style="font-size: 0;")
																		+IE-1({col: 2, width_col: '218', aling: 'center', valing: 'top'})
																		div(style="display:inline-block; width: 100%; max-width: 214px; vertical-align: top;")
